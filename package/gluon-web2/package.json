    "@types/jest": "^29.5.2",
    "eslint-config-ipfs": "^4.0.3",
    "jest": "^29.5.0",
    "parcel": "^2.9.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
