{
  "name": "gluon-web2",
  "version": "1.0.0",
  "description": "Gluon Web rewritten with Preact and Parcel",
  "author": "Gluon Contributors",
  "license": "MIT",
  "dependencies": {
    "preact": "^10.15.1"
  },
  "scripts": {
